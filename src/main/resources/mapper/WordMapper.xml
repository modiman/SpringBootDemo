<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.demo.mapper.WordMapper">
    <delete id="deleteWord">
        delete from word where wid = #{wid}
    </delete>

    <select id="getAllWords" resultType="com.springboot.demo.model.Word">
        select * from `word`
    </select>
</mapper>